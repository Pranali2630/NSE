
<div class="view-content">
   <table class="views-table cols-4" id="table">
      <thead>
         <tr>
            <th id="view-name-table-column" class="views-field views-field-name" scope="col">Date</th>
            <th id="view-email-table-column" class="views-field views-field-email" scope="col">Net Diff</th>
            <th id="view-mobile-table-column" class="views-field views-field-mobile" scope="col">FII Diff</th>
            <th id="view-mobile-table-column" class="views-field views-field-mobile" scope="col">DII Diff</th>
            <th id="view-mobile-table-column" class="views-field views-field-mobile" scope="col">Client Diff</th>
            <th id="view-mobile-table-column" class="views-field views-field-mobile" scope="col">Pro Diff</th>
         </tr>
      </thead>

      <tbody>
        {% for rec in items %}
         <tr>
            <td class="views-field views-field-id">{{ rec.0}}</td>
            
            <td headers="view-name-table-column" class="views-field views-field-name">{{ rec.1 }} </td>
            {% if rec.2 <= -1 %}
            <td headers="view-email-table-column" class="red views-field views-field-email">{{ rec.2 }}          </td>
            {% elseif rec.2 >= 1 %}
            <td headers="view-email-table-column" class="green views-field views-field-email">{{ rec.2 }}          </td>
            {% else %}
              <td headers="view-email-table-column" class="yellow views-field views-field-email"> {{ rec.2 }}          </td>
            {% endif %}


            {% if rec.3 <= -1 %}
            <td headers="view-email-table-column" class="red views-field views-field-email">{{ rec.3 }}          </td>
            {% elseif rec.3 >= 1 %}
            <td headers="view-email-table-column" class="green views-field views-field-email">{{ rec.3 }}          </td>
            {% else %}
              <td headers="view-email-table-column" class="yellow views-field views-field-email"> {{ rec.3 }}          </td>
            {% endif %}  


            {% if rec.4 <= -1 %}
            <td headers="view-email-table-column" class="red views-field views-field-email">{{ rec.4 }}          </td>
            {% elseif rec.4 >= 1 %}
            <td headers="view-email-table-column" class="green views-field views-field-email">{{ rec.4 }}          </td>
            {% else %}
              <td headers=" view-email-table-column" class="yellow views-field views-field-email"> {{ rec.4 }}          </td>
            {% endif %}   


           {% if rec.5 <= -1 %}
            <td headers="view-email-table-column" class="red views-field views-field-email">{{ rec.5 }}          </td>
            {% elseif rec.5 >= 1 %}
            <td headers="view-email-table-column" class="green views-field views-field-email">{{ rec.5 }}          </td>
            {% else %}
              <td headers="view-email-table-column" class="yellow views-field views-field-email"> {{ rec.5 }}          </td>
            {% endif %}  


          
         
         </tr>
        {% endfor %}
      </tbody>
   </table>
</div>

